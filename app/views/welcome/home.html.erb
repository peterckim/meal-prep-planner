<% flash.each do |key, value| %>
    <div class="flash <%= key %>"><%= value %></div>
<% end %>

<br>

<%= form_tag('/sessions') do %>
    <%= text_field_tag :email %>
    <%= password_field_tag :password %>
    <%= submit_tag "Login" %>
<% end %>
<%= link_to('Sign Up Here', new_user_path) %>
<%= link_to('Log in with Facebook!', '/auth/facebook') %>